        </Variable>
        <Variable name="usrAttr2" type="string">
            <JSONPath>$.user.picture</JSONPath>
        </Variable>
        <Variable name="usrAttr3" type="string">
            <JSONPath>$.user.activated</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">cred.response</Source>
</ExtractVariables>