<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Credentials">
    <DisplayName>Extract Credentials</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
        <Variable name="usr" type="string">
            <JSONPath>$.username</JSONPath>
        </Variable>
        <Variable name="pwd" type="string">
            <JSONPath>$.password</JSONPath>
        </Variable>
        <Variable name="grant" type="string">
            <JSONPath>$.grant_type</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">request</Source>
</ExtractVariables>